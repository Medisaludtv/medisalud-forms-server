{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "env": {
    "AWS_S3_MAX_FILE_SIZE": "50mb",
    "MAX_PAYLOAD_SIZE": "50mb"
  }
}